{
  "name": "Eksi Normallestirici",
  "manifest_version": 2,
  "version": "1.1.2",
  "description": "Ekşi Sözlük görünümünü Beta zamanına döndürür",
  "author": "Serdar Cevher",
  "permissions": ["https://eksisozluk.com/*", "https://eksisozluk2023.com/*"],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "name": "Eksi Normallestirici",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "background":{
       "scripts":[ "background.js" ]
   },
  "content_scripts": [
    {
      "matches": ["https://eksisozluk.com/*", "https://eksisozluk2023.com/*"],
      "js": ["jquery.min.js","script.js"],
      "css": ["fix.css"]
    }
  ]
}
